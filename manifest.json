{
  "manifest_version": 2,

  "name": "Image Switcher",
  "description": "Change all images in the tab!",
  "version": "1.0",
  "default_locale": "en",
  "icons": {
    "48": "icon.jpg"
  },

  "permissions": ["tabs", "<all_urls>"],

  "browser_action": {
    "default_icon": "icon.jpg",
    "default_popup": "popup.html"
  }
}