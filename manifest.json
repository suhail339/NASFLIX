{
  "manifest_version": 2,

  "name": "NAS FLIX",
  "description": "Clever Voice Commander",
  "version": "1.0",
  "default_locale": "en",
  "icons": {
    "19": "images/icon_19.png",
    "38": "images/icon_38.png",
    "128": "images/icon_128.png"
  },

  "permissions": ["tabs", "<all_urls>"],

  "browser_action": {
    "default_icon": {
      "19": "images/icon_19.png",
      "38": "images/icon_38.png"
    },
    "default_popup": "popup.html"
  },
  "homepage_url": "https://github.com/suhail339/NASFLIX.git"
}