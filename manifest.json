{
  "manifest_version": 2,

  "name": "Voice Commander",
  "description": "Navigate through websites.",
  "version": "1.0",
  "default_locale": "en",
  "icons": {
    "48": "icon.png"
  },

  "permissions": ["tabs", "<all_urls>"],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}